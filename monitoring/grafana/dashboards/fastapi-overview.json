{
  "id": null,
  "uid": "fastapi-overview",
  "title": "FastAPI Overview",
  "tags": ["fastapi", "prometheus"],
  "timezone": "browser",
  "schemaVersion": 30,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "graph",
      "title": "Average Request Duration (seconds)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "http_request_duration_seconds_sum / http_request_duration_seconds_count",
          "legendFormat": "Average Duration",
          "interval": ""
        }
      ],
      "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8}
    },
    {
      "type": "graph",
      "title": "Requests per Endpoint",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum by (handler) (http_requests_total)",
          "legendFormat": "{{handler}}",
          "interval": ""
        }
      ],
      "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8}
    },
    {
      "type": "graph",
      "title": "95th Percentile Request Duration (seconds)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, handler))",
          "legendFormat": "{{handler}}",
          "interval": ""
        }
      ],
      "gridPos": {"x": 0, "y": 8, "w": 24, "h": 8}
    }
  ]
}
